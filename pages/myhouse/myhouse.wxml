<view>
  <picker mode="date" value="{{date}}" start="2015-09-01" end="2017-09-01" bindchange="bindDateChange"> 
    <view>
      当前日期：{{date}}
    </view>
  </picker>
</view>

<picker mode="region" bindchange='bindRegionChange'>
    <view>
      当前选择：{{region}}
    </view>
</picker>