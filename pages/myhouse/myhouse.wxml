<!--pages/myhouse/myhouse.wxml-->
<swiper indicator-dots="true" autoplay="true" interval="1000" circular="true" indicator-active-color="#6600FF">
  <block wx:for="{{imgUrls}}" wx:key="*this" wx:item="item">
    <swiper-item >
      <image src="{{item}}" style="width:375px;height:240px"/>
    </swiper-item>
  </block>
</swiper>

<view>movable-view区域小于movable-area</view>
<movable-area style="width:375px;height:300px;background-color:#66CC33">

  <movable-view style="width:100px;height:50px;background-color:#6666CC" x="{{x}}" y="{{y}}" direction="all" inertia="true"  damping="10" out-of-bounds="true">

  </movable-view>
</movable-area>

